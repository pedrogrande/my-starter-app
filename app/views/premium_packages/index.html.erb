<h1>Listing premium_packages</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Price</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @premium_packages.each do |premium_package| %>
      <tr>
        <td><%= premium_package.user %></td>
        <td><%= premium_package.price %></td>
        <td><%= premium_package.customer_id %></td>
        <td><%= link_to 'Show', premium_package %></td>
        <td><%= link_to 'Edit', edit_premium_package_path(premium_package) %></td>
        <td><%= link_to 'Destroy', premium_package, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Premium package', new_premium_package_path %>
